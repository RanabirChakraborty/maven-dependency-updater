{
  "ignoreScopes": ["test"],

  "rules": {
    "com.google.code.gson:gson": {
      "PREFIX": "2.7"
    },
    "org.picketlink:*": {
      "PREFIX": "2.5.5",
      "QUALIFIER": "SP\\d+"
    },
    "javax.enterprise:cdi-api": {
      "PREFIX": "2.0",
      "QUALIFIER": ["", "SP\\d+"]
    },
    "org.apache.directory.server:apacheds-all": {
      "STREAM": "QUALIFIER"
    }
  }
}
